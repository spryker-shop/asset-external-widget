{
    "name": "spryker-shop/asset-external-widget",
    "type": "library",
    "description": "AssetExternalWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "spryker-shop/shop-cms-slot-extension": "^1.0.0",
        "spryker/asset-external-storage": "dev-beta/app-store-demo",
        "spryker/kernel": "^3.30.0",
        "spryker/store": "^1.8.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
